#For dependences
soft_dependences:
   - curl
   - ca-certificates
   - gnupg
   - apt-transport-https
modules:
   - overlay
   - br_netfilter

#For k8s
version: '1.26.0-00'
kube_soft_all:
   - kubelet={{ version }}
   - kubeadm={{ version }}
   - kubectl={{ version }}

docker_soft:
   - containerd.io
#repo
k8s_url: https://packages.cloud.google.com/apt/doc/apt-key.gpg
k8s_repo: deb https://apt.kubernetes.io/ kubernetes-xenial main
docker_url: https://download.docker.com/linux/{{ ansible_facts.distribution | lower }}/gpg
docker_repo: deb https://download.docker.com/linux/{{ ansible_facts.distribution | lower }} {{ ansible_facts.distribution_release }} stable
containerd_url: https://github.com/containerd/containerd/releases/download/v1.7.0/containerd-1.7.0-linux-amd64.tar.gz